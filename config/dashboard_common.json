{
    "config": {
        "getMetrics": {
            "interval": 30000,
            "hook": {
                "hostname": "localhost",
                "port": "3000",
                "path": "/metrics/har",
                "params": {
                    "connection": {
                        "downstreamKbps": 10000,
                        "upstreamKbps": 2000,
                        "latency": 10
                    },
                    "hookUrl": "http://localhost:9999/hook"
                }
            }
        },
        "pageLoadTimes": {
            "interval": 5000
        },
        "resourcesBySize": {
            "interval": 5000
        },
        "requestsByResources": {
            "interval": 5000
        },
        "psi": {
            "interval": 30000,
            "strategy": "desktop",
            "treshold": 75
        },
        "psi-mobile": {
            "interval": 30000,
            "strategy": "mobile",
            "treshold": 75
        }
    }
}
