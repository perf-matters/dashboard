{
  "layout": {
    "title": "My perf dashboard",
    "customJS": ["custom.js"],
    "gridSize" : { "columns" : 3, "rows" : 3 },
    "widgets": [
      {
        "enabled": true,
        "row": 0,
        "col": 0,
        "width": 0,
        "height": 0,
        "widget" : "getMetrics",
        "job" : "getMetrics",
        "config": "getMetrics"
      },
      {
        "enabled": true,
        "row": 1,
        "col": 1,
        "width": 3,
        "height": 1,
        "widget" : "pageLoadTimes",
        "job" : "pageLoadTimes",
        "config": "pageLoadTimes"
      },
      {
        "enabled": true,
        "row": 2,
        "col": 1,
        "width": 1,
        "height": 1,
        "widget" : "resourcesBySize",
        "job" : "resourcesBySize",
        "config": "resourcesBySize"
      },
      {
        "enabled": true,
        "row": 2,
        "col": 2,
        "width": 1,
        "height": 1,
        "widget" : "requestsByResources",
        "job" : "requestsByResources",
        "config": "requestsByResources"
      },
      {
        "enabled": true,
        "row": 2,
        "col": 3,
        "width": 1,
        "height": 1,
        "widget" : "psi",
        "job" : "psi",
        "config": "psi"
      },
      {
        "enabled": true,
        "row": 3,
        "col": 1,
        "width": 1,
        "height": 1,
        "widget" : "psi",
        "job" : "psi",
        "config": "psi-mobile"
      }
    ]
  },

  "config": {
    "getMetrics": {
      "interval": 30000
    },
    "pageLoadTimes": {
      "interval": 5000
    },
    "resourcesBySize": {
      "interval": 5000
    },
    "requestsByResources": {
      "interval": 5000
    },
    "psi": {
      "interval": 30000,
      "strategy": "desktop",
      "treshold": 75
    },
    "psi-mobile": {
      "interval": 30000,
      "strategy": "mobile",
      "treshold": 75
    }
  }
}
