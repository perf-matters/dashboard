{
  "layout": {
    "title": "My perf dashboard",
    "customJS": ["custom.js"],
    "gridSize" : { "columns" : 3, "rows" : 3 },
    "widgets": [
      {
        "enabled": true,
        "row": 0,
        "col": 0,
        "width": 0,
        "height": 0,
        "widget" : "getMetrics",
        "job" : "getMetrics",
        "config": "getMetrics"
      },
      {
        "enabled": true,
        "row": 1,
        "col": 1,
        "width": 3,
        "height": 1,
        "widget" : "pageLoadTimes",
        "job" : "pageLoadTimes",
        "config": "pageLoadTimes"
      },
      {
        "enabled": true,
        "row": 2,
        "col": 1,
        "width": 1,
        "height": 1,
        "widget" : "resourcesBySize",
        "job" : "resourcesBySize",
        "config": "resourcesBySize"
      }
    ]
  },

  "config": {
    "getMetrics": {
      "interval": 30000
    },
    "pageLoadTimes": {
      "interval": 5000
    },
    "resourcesBySize": {
      "interval": 5000
    }
  }
}
